{
  "name": "jamp3",
  "version": "0.4.1",
  "author": "ffalt",
  "license": "MIT",
  "description": "mp3, id3v1, id3v2 - reader & writer",
  "keywords": [
    "mp3",
    "ID3",
    "ID3v1",
    "ID3v2",
    "nodejs",
    "cli"
  ],
  "bin": {
    "mp3-analyze": "./bin/analyzeMP3.js",
    "id3v2-dump": "./bin/dumpID3v2.js",
    "id3v1-dump": "./bin/dumpID3v1.js",
    "id3v1-remove": "./bin/removeID3v1.js"
  },
  "scripts": {
    "build": "tsc -m commonjs --outDir dist",
    "build:deploy": "tsc -m commonjs --outDir dist",
    "test": "jest",
    "lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "lint-fix": "tslint --fix --project tsconfig.json \"src/**/*.ts\"",
    "docs": "typedoc --options ./typedocconfig.ts && cp ./.nojekyll ./docs",
    "coverage": "jest --coverage",
    "deploy": "deplokay -c deplokay.json",
    "deploy:docs": "deplokay -c deplokay-docs.json"
  },
  "dependencies": {
    "commander": "3.0.1",
    "debug": "4.1.1",
    "fs-extra": "8.1.0",
    "iconv-lite": "0.5.0",
    "memory-stream": "1.0.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/fs-extra": "8.0.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@types/tmp": "0.1.0",
    "deplokay": "0.0.9",
    "jest": "24.9.0",
    "tmp": "0.1.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typedoc": "0.15.0",
    "typescript": "3.6.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "src",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/jamp3.git"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
