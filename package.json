{
  "name": "jamp3",
  "version": "0.4.1",
  "author": "ffalt",
  "license": "MIT",
  "description": "mp3, id3v1, id3v2 - reader & writer",
  "keywords": [
    "mp3",
    "ID3",
    "ID3v1",
    "ID3v2",
    "nodejs",
    "cli"
  ],
  "bin": {
    "mp3-analyze": "./bin/analyzeMP3.js",
    "id3v2-dump": "./bin/dumpID3v2.js",
    "id3v1-dump": "./bin/dumpID3v1.js",
    "id3v1-remove": "./bin/removeID3v1.js"
  },
  "scripts": {
    "build": "tsc -m commonjs --outDir dist",
    "build:deploy": "tsc -m commonjs --outDir dist",
    "test": "jest",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "eslint --fix ./src --ext .js,.ts",
    "docs": "typedoc --options ./typedocconfig.ts && cp ./.nojekyll ./docs",
    "coverage": "jest --coverage",
    "deploy": "deplokay -c deplokay.json",
    "deploy:docs": "deplokay -c deplokay-docs.json"
  },
  "dependencies": {
    "commander": "5.1.0",
    "debug": "4.1.1",
    "fs-extra": "9.0.1",
    "iconv-lite": "0.6.2",
    "memory-stream": "1.0.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/fs-extra": "9.0.1",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@types/tmp": "0.2.0",
    "deplokay": "0.0.12",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "eslint": "7.4.0",
    "jest": "26.1.0",
    "tmp": "0.2.1",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "typedoc": "0.19.1",
    "typescript": "3.9.7"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "src",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/jamp3.git"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
